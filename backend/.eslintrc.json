{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi":"warn",
        "array-bracket-spacing":"warn",
        "camelcase": "warn",
        "quotes": [ "warn", "single" ],
        "complexity": "error",
        "no-duplicate-imports": "error"
        
    }
}
